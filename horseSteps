#include <iostream>
#include <memory.h>
#include <algorithm>
#include <vector>
#include <stdio.h>
using namespace std;


const int MAXN = 10;
const int CONFIG = 6;
const int CON = CONFIG * CONFIG;

struct Node {
	int x;
	int y;
	int totalToNext;
};
int map[MAXN][MAXN];
int path[CON+5];
int move_x[] = {1, -1, 1, -1, 2, -2, 2, -2};
int move_y[] = {-2, -2, 2, 2, -1, -1, 1, 1};
bool cmp(Node , Node );
bool isValid(Node );
bool dfs(Node ,int );

int main() {
	Node s;
	Node check;
	int N;
	int i;
	int flag=0;
	//int count=0;
	memset(map, 0, sizeof(int)*MAXN*MAXN);
	cout << "Please enter the number of start(e.g. row2, column3 means 9):" << endl;
	freopen("C:\\Users\\Will\\Desktop\\test.txt", "w", stdout);  
	//while (cin >> N && N != -1) {
		cin >> N;
		cout << "Here is the solution for location " << N << ": " << endl;
		s.x = (N-1) / CONFIG + 1;
		s.y = (N-1) % CONFIG + 1;
		//path[0] = N;
		map[s.x][s.y] = 1;
		dfs(s, 1);
		for (i=0; i<8; i++) {
			check.x = s.x + move_x[i];
			check.y = s.y + move_y[i];
			if (check.x ==  (path[CON-2] -1) / CONFIG + 1 &&
			check.y == (path[CON-2] -1) % CONFIG + 1)
				flag =1;
		}
		if (!flag) cout << endl << "The largest steps is 35." << endl << endl;
		else cout << "(" << s.x << ", " << s.y << ")  ENTER" << endl << endl; 
		
	//}
	
	return 0;
}

bool dfs(Node n, int current) {
	int i,j;
	Node p,q;
	vector<Node> tmp;
	if (current == CON) {
		for (i=0; i<CONFIG; i++) {
			for (j=0; j<CONFIG; j++) {
				if (i==CONFIG-1 && j==CONFIG-1) break;
				cout << "(" << (path[i*CONFIG+j]-1) / CONFIG + 1 <<
				", " << (path[i*CONFIG+j]-1) % CONFIG +1 << ") ";
			}
			if (i==CONFIG-1 && j==CONFIG-1) continue; 
			cout <<  " ENTER" << endl;
			
		}
		return true;
	}
	else {
		for (i=0; i<8; i++) {
			p.x = n.x + move_x[i];
			p.y = n.y + move_y[i];
			p.totalToNext = 0;
			
			if (isValid(p)) {
				for (j=0; j<8; j++) {
					q.x = p.x + move_x[j];
					q.y = p.y + move_y[j];
					if (isValid(q))
						p.totalToNext++;
				}
				tmp.push_back(p);
			}
		}
		sort(tmp.begin(), tmp.end(), cmp);
		for (i=0; i<tmp.size(); i++) {
			map[tmp[i].x][tmp[i].y] = 1;
			path[current-1] = (tmp[i].x - 1)* CONFIG + tmp[i].y;
			if (dfs(tmp[i], current+1)) return true;
			map[tmp[i].x][tmp[i].y] = 0;
		}
	}
	return false;
}
bool isValid(Node n) {
	return (n.x>=1 && n.x<=CONFIG && n.y>=1 && n.y<=CONFIG && !map[n.x][n.y]);
}
bool cmp(Node a, Node b) {
	return (a.totalToNext < b.totalToNext);
}
